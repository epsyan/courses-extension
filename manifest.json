{
  "name": "Alfa Course",
  "version": "1.0",
  "description": "Alfa course!",
  "manifest_version": 2,
  "background": {
    "page": "public/background.html",
    "persistent": true
  },

  "browser_action": {
    "default_popup": "public/popup.html",
    "default_icon": {
      "16": "public/asset/icon-16.png"
    }
  },

  "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'"
}
